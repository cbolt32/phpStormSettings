<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1300" />
    <option name="IS_BLOCK_CURSOR" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="ffffff" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\btodo-toadd">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\btodo-Chris">
      <option name="FOREGROUND" value="c015" />
      <option name="BACKGROUND" value="ffffff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="TODO">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <filter name="Work to Add">
      <pattern index="0" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="10" />
    <option name="tagTreeHighlightingOpacity" value="40" />
  </component>
</application>